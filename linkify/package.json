{
	"name": "linkify",
	"displayName": "Linkify",
	"description": "Format links copied from Tabs Outliner for Markdown",
	"publisher": "drkvogel",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.linkify"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "extension.linkify",
			"title": "Linkify"
		}]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
